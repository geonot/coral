{
    "comments": {
        "lineComment": "#"
    },
    "brackets": [
        ["[", "]"],
        ["(", ")"],
        ["{", "}"]
    ],
    "autoClosingPairs": [
        ["[", "]"],
        ["(", ")"],
        ["{", "}"],
        ["'", "'"]
    ],
    "surroundingPairs": [
        ["[", "]"],
        ["(", ")"],
        ["{", "}"],
        ["'", "'"]
    ],
    "indentationRules": {
        "increaseIndentPattern": "^\\s*(fn|object|store|if|else|for|while)\\b.*$",
        "decreaseIndentPattern": "^\\s*(else|return)\\b.*$"
    },
    "wordPattern": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b"
}